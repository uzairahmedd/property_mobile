{
  "expo": {
    "name": "property_app",
    "slug": "property_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.app.khiaratee"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to able you to  upload property images."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "maplibre",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoicmFrYW5vbmxpbmUiLCJhIjoiY2xkMm45ZHlsMGE1azNvbWw4ZzJpc2cyYiJ9.7DNkDsOZsQcqJNWyTdIJCw"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b822c1c1-173f-4d28-b1cd-c5861cdfbd18"
      }
    }
  }
}
